{
  "manifest_version": 2,

  "name": "BikeNChill",
  "description": "Bike BIKE Bike",
  "version": "1.0",
  "browser_action": {
    "default_icon": "rsz_bike.png",
    "default_popup": "popup.html"
  },
  "background": { 
   "page":"bg.html"
  },
  "options_page": "options.html",
  "content_scripts": [{
    "matches":[
      "<all_urls>"
    ],
    "js":["https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js", "script.js", "url-pattern.js"],
    "js": ["options.js","block.js"]
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://peaceful-reef-6842.herokuapp.com/bikeSession/getjson*"
  ]
}
